<Addin
	id="Launchpad"
	namespace="Do"
	version="1.1"
	name="Launchpad"
	description="Provides integration with Launchpad.net."
	author="Ken Simon, David Siegel"
>
 
	<Runtime>
		<Import assembly="Launchpad.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>
	
	<Extension path= "/Do/Action">
		<Action type="Launchpad.LaunchpadAction" />
	</Extension>
</Addin>
