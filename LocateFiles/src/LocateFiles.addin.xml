<Addin
	id="LocateFiles"
	namespace="Do"
	version="1.0"
	name="Locate Files"
	description="Find files on your computer using the GNU locate command."
	author="David Siegel"
	category="Official"
>
 
	<Runtime>
		<Import assembly="LocateFiles.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>
	
	<Extension path= "/Do/Action">
		<Action type="Do.Universe.LocateFilesAction" />
	</Extension>
</Addin>
