<Addin 
	id="TrackerSearch"
	namespace="Do"
	version="1.0"
	name="Tracker Search"
	description="Initiate Tracker searches."
	author="Katriel Traum"
	category="Community"
	defaultEnabled="false"
	>

	<Runtime>
		<Import assembly="TrackerSearch.dll"/>
	</Runtime>

	<Localizer type="Gettext" catalog="gnome-do-plugins" location="@expanded_datadir@/locale" />

	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>

	<Extension path="/Do/Action"> 
		<Action type="TrackerSearch.TrackerSearchAction" />
	</Extension>
</Addin>
