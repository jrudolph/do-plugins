<Addin 
	id="TrackerSearch"
	namespace="Do"
	version="1.0"
	name="Tracker Search"
	description="Initiate Tracker searches."
	author="Katriel Traum"
	category="Community"
	>

	<Runtime>
		<Import assembly="TrackerSearch.dll"/>
	</Runtime>

	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>

	<Extension path="/Do/Action"> 
		<Action type="TrackerSearch.TrackerSearchAction" />
	</Extension>
</Addin>
