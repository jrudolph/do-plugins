<Addin 
	id="Confluence" 
	namespace="Do"
	version="1.0"
	name="Confluence"
	description="Interact with Confluence wikis."
	author="John Wood"
	category="Community"
	>

	<Runtime>
		<Import assembly="Confluence.dll"/>
	</Runtime>

	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>

	<Extension path = "/Do/Action">
		<Action type="Confluence.ConfluenceSearchAction" />
	</Extension>

</Addin>
