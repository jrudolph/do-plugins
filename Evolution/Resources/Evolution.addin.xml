<Addin 
	id="Evolution"
	namespace="Do"
	version="1.2"
	name="Evolution"
	description="Search your Evolution contacts."
	author="David Siegel"
	category="Official"
>
 
	<Runtime>
		<Import assembly="Evolution.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>
	
	<Extension path="/Do/ItemSource">
		<ItemSource type="Evolution.ContactItemSource" />
	</Extension>
</Addin>
