<Addin 
	id="SqueezeCenter" 
	namespace="Do"
	version="1.1"
	name="SqueezeCenter"
	description="Controls Logitech Squeezebox and Transport audio players."
	author="Anders Thomsen"
	category="Community"
	>

	<Runtime>
		<Import assembly="SqueezeCenter.dll"/>
	</Runtime>

	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>

	<!-- Extensions included in this assembly -->
	<Extension path="/Do/Action">
		<Action type="SqueezeCenter.Enqueue" />
		<Action type="SqueezeCenter.Play" />
		<Action type="SqueezeCenter.TurnOn" />
		<Action type="SqueezeCenter.TurnOff" />
		<Action type="SqueezeCenter.Pause" />
		<Action type="SqueezeCenter.Sync" />
		<Action type="SqueezeCenter.Unsync" />
		<Action type="SqueezeCenter.Prev" />
		<Action type="SqueezeCenter.Next" />
	</Extension>

	<Extension path="/Do/ItemSource">
		<ItemSource type="SqueezeCenter.ItemSource" />
	</Extension>

</Addin>
