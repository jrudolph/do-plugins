<Addin
	id="Pastebin"
	namespace="Do"
	version="1.0"
	name="Pastebin"
	description="Paste text to an online pastebin."
	author="Jason Jones"
>
 
	<Runtime>
		<Import assembly="Pastebin.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Core" version="1.0" />
	</Dependencies>
	
	<!-- Extensions included in this assembly -->
	<!-- Sources -->
	<Extension path= "/Do/Action">
		<Action type="Pastebin.PastebinAction" />
	</Extension>
</Addin>
