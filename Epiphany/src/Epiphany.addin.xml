<Addin 
	id="Epiphany"
	namespace="Do"
	version="1.0"
	name="Epiphany"
	description="Indexes your Epiphany bookmarks"
	author="David Siegel"
>
 
	<Runtime>
		<Import assembly="Epiphany.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Core" version="1.0" />
	</Dependencies>
	
	<!-- Extensions included in this assembly -->
	<!-- Sources -->
	<Extension path = "/Do/ItemSource">
		<ItemSource type="Epiphany.EpiphanyBookmarkItemSource" />
	</Extension>
</Addin>
