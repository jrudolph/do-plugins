<Addin
	id="Firefox"
	namespace="Do"
	version="2.1"
	name="Firefox"
	description="Search Firefox 3 bookmarks."
	author="David Siegel"
	category="Official"
	>

	<Runtime>
		<Import assembly="Firefox.dll"/>
	</Runtime>

	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>

	<Extension path="/Do/ItemSource">
		<ItemSource type="Mozilla.Firefox.BookmarkItemSource" />
	</Extension>
</Addin>
