<Addin
	id="Firefox"
	namespace="Do"
	version="3.0"
	name="Firefox"
	description="Search Firefox 3 bookmarks, bookmark directories, and history."
	author="David Siegel, Neal Stewart, Alex Launi"
	category="Official"
	defaultEnabled="false"
	>

	<Runtime>
		<Import assembly="Firefox.dll"/>
	</Runtime>

	<Localizer type="Gettext" catalog="gnome-do-plugins" location="/home/chris/.local/share/locale" />

	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>

	<Extension path="/Do/ItemSource">
		<ItemSource type="Firefox.PlacesItemSource" />
	</Extension>
</Addin>
