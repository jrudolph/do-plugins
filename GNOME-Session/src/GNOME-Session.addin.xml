<Addin
	id="GNOME Session and Power Management"
	namespace= "Do"
	version="1.1"
	name="GNOME Session and Power Management"
	description="Log out, lock your screen, shutdown, hibernate, etc."
	author="David Siegel"
>
 
	<Runtime>
		<Import assembly="GNOME-Session.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Core" version="1.0" />
	</Dependencies>
	
	<!-- Extensions included in this assembly -->
	<!-- Sources -->
	<Extension path = "/Do/ItemSource">
		<ItemSource type="GNOME.Session.SessionCommandsItemSource" />
	</Extension>
</Addin>
