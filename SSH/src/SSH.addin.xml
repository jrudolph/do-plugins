<Addin
	id="SSH"
	namespace="Do"
	version="1.0"
	name="SSH"
	description="Search and connect to SSH hosts."
	author="Peter Stuifzand"
	category="Community"
	>

	<Runtime>
		<Import assembly="SSH.dll"/>
	</Runtime>

	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>

	<Extension path = "/Do/ItemSource">
		<ItemSource type="GnomeDoSSH.SSHHostItemSource" />
	</Extension>
	<Extension path = "/Do/Action">
		<Action type="GnomeDoSSH.SSHAction" />
	</Extension>
</Addin>
