<Addin
	id="SSH"
	namespace="Do"
	version="1.0"
	name="SSH"
	description="Indexes ssh hosts and provides 'Connect with SSH' action"
	author="Peter Stuifzand"
	category="Community"
	>

	<Runtime>
		<Import assembly="SSH.dll"/>
	</Runtime>

	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>

	<!-- Extensions included in this assembly -->
	<!-- Sources -->
	<Extension path = "/Do/ItemSource">
		<ItemSource type="GnomeDoSSH.SSHHostItemSource" />
	</Extension>
	<Extension path = "/Do/Action">
		<Action type="GnomeDoSSH.SSHAction" />
	</Extension>
</Addin>
