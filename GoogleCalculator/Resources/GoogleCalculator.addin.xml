<Addin
	id="GoogleCalculator"
	namespace= "Do"
	version="1.0"
	name="Google Calculator"
	description="Perform calculations, and currency/quantity conversions."
	author="David Siegel"
	category="Official"
	defaultEnabled="false"
>
 
	<Runtime>
		<Import assembly="GoogleCalculator.dll"/>
	</Runtime>

	<Localizer type="Gettext" catalog="gnome-do-plugins" location="/home/chris/.local/share/locale" />
	
	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>
	
	<Extension path= "/Do/Action">
		<Action type="Do.Plugins.Google.GoogleCalculatorAction" />
	</Extension>
</Addin>
