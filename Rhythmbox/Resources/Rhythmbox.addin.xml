<Addin
	id="Rhythmbox"
	namespace="Do"
	version="2.0"
	name="Rhythmbox"
	description="Searches and plays Rhythmbox music."
	author="David Siegel"
	>

	<Runtime>
		<Import assembly="Rhythmbox.dll"/>
	</Runtime>

	<Dependencies>
		<Addin id="Core" version="1.0" />
	</Dependencies>

	<!-- Extensions included in this assembly -->
	<!-- Sources -->
	<Extension path = "/Do/ItemSource">
		<ItemSource type="Do.Rhythmbox.MusicItemSource" />
	</Extension>
	<Extension path= "/Do/Action">
		<Action type="Do.Rhythmbox.EnqueueAction" />
		<Action type="Do.Rhythmbox.PlayAction" />
	</Extension>
</Addin>
