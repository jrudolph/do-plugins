<Addin
	id="Shelf"
	namespace="Do"
	version="1.0"
	name="Shelf"
	description="Store your most used items with Shelf."
	author="Jason Smith"
	>

	<Runtime>
		<Import assembly="Shelf.dll"/>
	</Runtime>

	<Dependencies>
		<Addin id="Core" version="1.0" />
	</Dependencies>

	<!-- Extensions included in this assembly -->

	<Extension path="/Do/ItemSource">
		<ItemSource type="Do.Universe.ShelfItemSource" />
	</Extension>

	<!-- Actions -->
	<Extension path="/Do/Action">
		<Action type="Do.Universe.ShelfAddAction" />
		<Action type="Do.Universe.ShelfExploreAction" />
		<Action type="Do.Universe.ShelfRemoveAction" />
	</Extension>

</Addin>
