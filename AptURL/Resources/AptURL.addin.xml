<Addin 
	id="AptURL"
	namespace="Do"
	version="1.0"
	name="AptURL Package Installer"
	description="Install apt packages by name."
	author="Christer Edwards"
	category="Community"
	defaultEnabled="false"
	>

	<Runtime>
		<Import assembly="AptURL.dll"/>
	</Runtime>

	<Localizer type="Gettext" catalog="gnome-do-plugins" location="/home/chris/.local/share/locale" />

	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>

	<Extension path="/Do/Action">
		<Action type="AptURL.AptAction" />
	</Extension>

</Addin>
