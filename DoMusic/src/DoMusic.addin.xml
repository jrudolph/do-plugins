<Addin id="DoMusic" namespace="Do" version="1.0" name="Do Music" description="Host for Lots of Music Plugins" author="Zach Goldberg">
 
	<Runtime>
		<Import assembly="DoMusic.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Core" version="1.0" />
	</Dependencies>

	<Extension path= "/Do/ItemSource">
		<ItemSource type="Do.Addins.DoMusic.ControlActionSource" />
	</Extension>
	
	<Extension path= "/Do/ItemSource">
		<ItemSource type="Do.Addins.DoMusic.MusicItemSource" />
	</Extension>
	
	<Extension path= "/Do/Action">
		<Action type="Do.Addins.DoMusic.ControlActionPlay" />
	</Extension>
	
	<Extension path= "/Do/Action">
		<Action type="Do.Addins.DoMusic.ControlActionPauseResume" />
	</Extension>
	
	<Extension path= "/Do/Action">
		<Action type="Do.Addins.DoMusic.ControlActionNext" />
	</Extension>
	
	<Extension path= "/Do/Action">
		<Action type="Do.Addins.DoMusic.ControlActionPrevious" />
	</Extension>
	
	<Extension path= "/Do/Action">
		<Action type="Do.Addins.DoMusic.DoMusicPlayAction" />
	</Extension>
	
	<Extension path= "/Do/Action">
		<Action type="Do.Addins.DoMusic.DoMusicEnqueueAction" />
	</Extension>
	
	<Extension path= "/Do/Action">
		<Action type="Do.Addins.DoMusic.SetDefaultAction" />
	</Extension>	
	
</Addin>
