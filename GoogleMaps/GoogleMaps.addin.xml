<Addin 
	id="GoogleMaps"
	namespace="Do"
	version="1.0"
	name="Google Maps"
	description="Map a location or route in Google maps."
	author="DR Colkitt"
>
 
	<Runtime>
		<Import assembly="GoogleMaps.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>
	
	<!-- Extensions included in this assembly -->
	<Extension path="/Do/Action">
		<Action type="Google.MapAction" />
	</Extension>

</Addin>
