<Addin
	id="TinyUrl"
	namespace="Do"
	version="1.0"
	name="TinyUrl"
	description="Make unwieldy urls tiny."
	author="David Siegel"
	category="Official"
	defaultEnabled="false"
	>

	<Runtime>
		<Import assembly="TinyUrl.dll"/>
	</Runtime>

	<Dependencies>
		<Addin id="Universe" version="1.0" />
	</Dependencies>

	<Extension path="/Do/Action">
		<Action type="TinyUrl.MakeUrlTinyAction" />
	</Extension>
</Addin>
