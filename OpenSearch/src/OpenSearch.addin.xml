<Addin
	id="OpenSearch"
	namespace= "Do"
	version="1.0"
	name="OpenSearch Web Search izzle"
	description="Search the web using your installed OpenSearch plugins."
	author="Jason Jones"	
>
 
	<Runtime>
		<Import assembly="OpenSearch.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Core" version="1.0" />
	</Dependencies>
	
	<!-- Extensions included in this assembly -->
	<Extension path="/Do/Action">
		<Action type="OpenSearch.OpenSearchAction" />
	</Extension>

	<Extension path="/Do/ItemSource">
		<ItemSource type="OpenSearch.OpenSearchItemSource" />
	</Extension>
</Addin>
