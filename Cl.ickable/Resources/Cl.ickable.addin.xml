<Addin
	id="Cl.ickable"
	namespace="Do"
	version="1.0"
	name="Cli.ickable WebClips"
	description="Create WebClips with Cl.ickable."
	author="Idealab"
>
 
	<Runtime>
		<Import assembly="Cl.ickable.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Core" version="1.0" />
	</Dependencies>
	
	<!-- Extensions included in this assembly -->
	<!-- Sources -->
	<Extension path="/Do/Action">
		<Action type="Cl.ickable.ClipAction" />
	</Extension>

	<Extension path="/Do/ItemSource">
		<ItemSource type="Cl.ickable.ClickableItemSource" />
	</Extension>
</Addin>
