<Addin 
	id="Banshee"
	namespace= "Do"
	version="0.5"
	name="Banshee"
	description="Control Banshee Media Player playback"
	author="Alex Launi"
	category="Official"
>
 
	<Runtime>
		<Import assembly="Banshee.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Platform" version="1.0" />
		<Addin id="Universe" version="1.0" />
	</Dependencies>
	
	<!-- Extensions included in this assembly -->

	<!-- Actions -->
	<Extension path = "/Do/Action">
		<Action type = "Banshee.PlayAction" />
	</Extension>
	<Extension path = "/Do/Action">
		<Action type = "Banshee.PauseAction" />
	</Extension>
	<Extension path = "/Do/Action">
		<Action type = "Banshee.NextAction" />
	</Extension>
	<Extension path = "/Do/Action">
		<Action type = "Banshee.PreviousAction" />
	</Extension>
</Addin>
