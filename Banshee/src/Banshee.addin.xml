<Addin 
	id="Banshee"
	namespace= "Do"
	version="1.0"
	name="Banshee"
	description="Searches and plays Banshee music"
	author="Mathieu Cadet"
>
 
	<Runtime>
		<Import assembly="Banshee.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Core" version="1.0" />
	</Dependencies>
	
	<!-- Extensions included in this assembly -->
	<!-- Sources -->
	<Extension path = "/Do/ItemSource">
		<ItemSource type="Do.Addins.Banshee.BansheeItemSource" />
	</Extension>
	<Extension path = "/Do/Action">
		<Action type="Do.Addins.Banshee.BansheePlayAction" />"
	</Extension>
</Addin>
